<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Form</title>
</head>
<body>
  <h1>Form</h1>
  <form action="/submit" method="POST">
    <label for="cost">Cost:</label>
    <input type="text" id="cost" name="cost" required><br><br>
    <label for="date">Date:</label>
    <input type="date" id="date" name="date" required><br><br>
    <label for="from_time">From Time:</label>
    <select id="from_time" name="from_time" required>
      <% for (var i = 1; i <= 24; i++) { %>
        <option value="<%= i %>"><%= i %></option>
      <% } %>
    </select><br><br>
    
    <label for="to_time">To Time:</label>
    <select id="to_time" name="to_time" required>
      <% for (var i = 1; i <= 24; i++) { %>
        <option value="<%= i %>"><%= i %></option>
      <% } %>
    </select><br><br>
    <label for="interest">Interest:</label>
    <select id="interest" name="interest" multiple>
      <option value="Cultural Monument">Cultural Monument</option>
      <option value="Eat and Drink">Eat and Drink</option>
      <option value="Historical Monument">Historical Monument</option>
      <option value="City Experience">City Experience</option>
      <option value="Nature">Nature</option>
      <option value="Art and Culture">Art and Culture</option>
      <option value="Museum">Museum</option>
    </select><br><br>
    <label for="accessibility">Accessibility:</label>
    <select id="accessibility" name="accessibility" required>
      <option value="true">True</option>
      <option value="false">False</option>
    </select><br><br>
    <button type="submit">Submit</button>
  </form>
</body>
</html>
